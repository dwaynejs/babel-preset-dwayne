{
  "name": "babel-preset-dwayne",
  "version": "5.0.1",
  "main": "lib/index.js",
  "devDependencies": {
    "babel-cli": "^6.24.1",
    "babel-core": "^6.25.0",
    "babel-plugin-syntax-jsx": "^6.18.0",
    "babel-preset-es2015": "^6.24.1",
    "mocha": "^3.4.2",
    "nyc": "^11.0.2"
  },
  "scripts": {
    "build": "babel src --out-dir lib",
    "test": "nyc mocha test/index.js --reporter dot",
    "prepublish": "npm test && npm run build"
  },
  "nyc": {
    "check-coverage": true,
    "lines": 100,
    "statements": 100,
    "functions": 100,
    "branches": 100,
    "sourceMap": true,
    "include": [
      "src/**/*.js"
    ],
    "exclude": [
      "test/**/*.js",
      "node_modules/**/*"
    ],
    "reporter": [
      "html"
    ],
    "cache": true,
    "all": true
  },
  "dependencies": {
    "babel-plugin-transform-dwayne-html": "^5.0.0",
    "babel-plugin-transform-dwayne-js": "^5.0.0",
    "babel-plugin-transform-dwayne-jsx": "^5.0.0",
    "lodash": "^4.17.4"
  }
}
